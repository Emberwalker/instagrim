<configuration>
    <contextName>Instagrim</contextName>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{HH:mm:ss}][%thread][%logger{36}][%method:%line][%-5level]: %msg%n</pattern>
        </encoder>
    </appender>

    <root level="debug">
        <appender-ref ref="STDOUT" level="debug"/>
    </root>
</configuration>